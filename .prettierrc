{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 120,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "plugins": ["@prettier/plugin-php"],
    "overrides": [
        {
            "files": "*.blade.php",
            "options": {
                "parser": "php"
            }
        }
    ]
}
